// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id        String    @id @default(uuid())
  file      String
  filePath  String
  size      Int
  createdAt DateTime?
  updatedAt DateTime?
}
